<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ArtbavialFinance.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pages="clr-namespace:ArtbavialFinance.Pages"
             xmlns:data="clr-namespace:ArtBavialFinance"
    Shell.FlyoutBehavior="Disabled"
    Title="ArtbavialFinance">


    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate data:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Currencies"
        ContentTemplate="{DataTemplate pages:ManageCurrenciesPage}"
        Route="currencies" />

</Shell>

